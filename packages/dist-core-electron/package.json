{
    "name": "@nvl-maker/dist-core-electron",
    "version": "0.0.1",
    "description": "Core electron distribution.",
    "repository": "git@github.com:cocoloring/nvl-maker.git",
    "keywords": [
        "typescript"
    ],
    "author": "cocoloring <cocoloring@outlook.com>",
    "license": "MIT",
    "main": "build/main.js",
    "types": "build/main.d.ts",
    "scripts": {
        "build": "yarn run-s build:* && yarn create-package-json",
        "build:webpack": "yarn webpack",
        "dev": "yarn yarn run-p dev:*",
        "dev:webpack": "yarn webpack --watch",
        "dev:electron": "yarn create-package-json && yarn electron build",
        "create-package-json": "node scripts/create-package-json.js",
        "clean": "yarn run-p clean:*",
        "clean:node": "yarn rimraf node_modules",
        "clean:build": "yarn rimraf build tsconfig.tsbuildinfo"
    },
    "dependencies": {
        "@nvl-maker/dist-core-electron-main": "0.0.1",
        "@nvl-maker/dist-core-electron-preload": "0.0.1",
        "@nvl-maker/dist-core-electron-renderer": "0.0.1"
    },
    "devDependencies": {
        "electron": "^13.2.2"
    }
}
