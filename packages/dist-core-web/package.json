{
    "name": "@nvl-maker/dist-core-web",
    "version": "0.0.1",
    "description": "Web distribution for core.",
    "repository": "git@github.com:cocoloring/nvl-maker.git",
    "keywords": [
        "typescript",
        "react",
        "webpack"
    ],
    "author": "cocoloring <cocoloring@outlook.com>",
    "license": "MIT",
    "main": "build/main.js",
    "types": "build/main.d.ts",
    "scripts": {
        "build": "yarn run-s build:*",
        "build:webpack": "yarn webpack --config webpack.config.js",
        "dev": "yarn yarn run-p dev:*",
        "dev:pack": "yarn webpack serve --open --config webpack.dev.config.js",
        "clean": "yarn run-p clean:*",
        "clean:node": "yarn rimraf node_modules",
        "clean:build": "yarn rimraf build"
    },
    "dependencies": {
        "react": "^17.0.2",
        "react-dom": "^17.0.2"
    },
    "devDependencies": {
        "@types/react": "^17.0.14",
        "@types/react-dom": "^17.0.9",
        "@types/webpack": "^5.28.0",
        "html-webpack-plugin": "^5.3.2",
        "ts-loader": "^9.2.3",
        "typescript": "^4.3.5",
        "webpack": "^5.45.1",
        "webpack-cli": "^4.7.2",
        "webpack-dev-server": "^3.11.2"
    }
}
